<div class="side-bar">



    <a href="/" <% if(pageName === 'index') { %> class="side-bar__active" <% } %>>
        <div class="side-bar__item">
            <img src="/img/icons/home.svg"/>
            <p>Home</p>
        </div>
    </a>

<% pages.forEach(function(item) { %>

 <a href="/page/<%= item.pageid %>">
        <div class="side-bar__item">
            <img src="<%= item.icon %>"/>
            <p><%= item.title %> </p>
        </div>
    </a>

            <% }); %>


    <a href="/login" <% if(pageName === 'admin') { %> class="side-bar__active" <% } %>>
        <div class="side-bar__item">
            <img class="side-bar__item__lock" src="/img/icons/login.svg"/>
            <p>Login</p>
        </div>
    </a>
</div>

<script>
    <% pages.forEach(function(item) { %>
       <%= item %>
    <% }); %>
</script>